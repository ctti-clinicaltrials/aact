  <brief_title>Effects of Lutein Supplementation on Subclinical Atherosclerosis</brief_title>
  <official_title>The Effects of Lutein Supplementation on Subclinical Atherosclerosis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Peking University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
