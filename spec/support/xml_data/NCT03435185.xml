  <brief_title>Greater Occipital and Supraorbital Nerve Blockade in Migraine Patients</brief_title>
  <official_title>Greater Occipital and Supraorbital Nerve Blockade For The Preventive Treatment of Migraine</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Dr.Lütfi Kırdar Kartal Eğitim ve Araştırma Hastanesi</agency>
      <agency_class>Other</agency_class>
